e = c%100
f = e//4

g = e//100

if g%4 == 0 :
    h = 6
elif g%4 == 1:
    h = 4
elif g%4 == 2:
    h = 2
else:
    h = 0

x = a+d+e+f+h

if x%7==0:
    print("Hari Sabtu")
elif x%7==1:
    print("Hari Minggu")
elif x%7 == 2:
    print("Hari Senin")
elif x%7 == 3:
    print("Hari Selasa")
elif x%7 == 4:
    print("Hari Rabu ")
elif x%7 == 5:
    print("Hari Kamis ")
else:
    print("Hari Jumat ")









  
        
      
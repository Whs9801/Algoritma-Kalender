a = int(input("Masukkan tanggal "))
b = int(input("Masukkan bulan "))
c = int(input("Masukkan tahun "))

if b == 1:
    if a>31:
        print("Input salah ")
    
    else:
        if c%400==0 or c%4==0 and c%100 != 0:
            d = 0
        else:
            d = 1
    
elif b == 2:
    if c%4==0:
        if a>29:
            print("Input salah ")
        else:
            if c%400==0 or c%4==0 and c%100 != 0:
              d = 3
            else:
              d = 4
    
    else:
        if a>28:
            print("Input salah ")
        else:
            if c%400==0 or c%4==0 and c%100 != 0:
              d = 3
            else:
              d = 4

elif b == 3:
    if a>31:
        print("Input salah ")
    else:
        d = 4

